{
    "compilerOptions": {
        "target": "ES2015",
        "module": "CommonJS",
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noEmitOnError": true,
        "noImplicitAny": true,
        "baseUrl": ".",
        "paths": {
            "instead-js/*": [
                "lib/instead-js/*"
            ]
        }
    },
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Recommended"
}